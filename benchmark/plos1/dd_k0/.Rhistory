growthsimulation <- function(popsize, prob, maxpop, times){#
x <- rep(NA,times)#
  for (j in 1:times){#
    counter<- 0#
     while ( popsize[length(popsize)] < maxpop ){#
      newpop <- rgeom( n = popsize[length(popsize)], prob);#
        currpopsize <- sum(newpop);#
          popsize <- c(popsize, currpopsize);    #
      counter <- counter+1#
      if (popsize[length(popsize)] <= 0){#
      break;#
      times <- times; maxpop <- maxpop; prob <- prob; popsize <- popsize;#
      }#
   x[j] <- c(length(popsize))     #
}#
#
}#
#
z <- sum(is.na(x))#
cat('There were', z, 'NAs\n')#
return(x)#
}
